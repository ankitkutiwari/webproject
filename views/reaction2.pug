.container(style='display: flex;')
  // table, table-borderless

  form(method='GET' action='/balance2')
    table
      tr
        td
         input(type='number' min='1' step='1' name='S' required)
        td #{datas.A}
 
        td +
 
    
        td
          input(type='number'  min='1' step='1' name='SO' required)
        td #{datas.B}

        td +

        td
          input(type='number'  min='1' step='1' name='AV' required)
        td #{datas.C}
 
        td ------->>>

        td
          input(type='number' min='1' step='1' name='MR' required)
        td #{datas.D}

      tr
        td(colspan='2')
          input.btn.btn-primary(type='submit' value='Submit') 



