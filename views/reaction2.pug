body 
  // Navbar begins
  header#header.header-effect-shrink(data-plugin-options="{'stickyEnabled': true, 'stickyEffect': 'shrink', 'stickyEnableOnBoxed': true, 'stickyEnableOnMobile': true, 'stickyChangeLogo': true, 'stickyStartAt': 30, 'stickyHeaderContainerHeight': 70}")
    .header-body.header-body-bottom-border-fixed.bg-blue-chan
      .header-container.container
        .header-row
          .header-column
            .header-row
              .header-logo
                a(href='/')
                  img(alt='Chemgineers' width='40' data-sticky-width='40' data-sticky-height='40' src='Icon ionic-logo-buffer.png')
          .header-column.justify-content-end
            .header-row
              .header-nav.header-nav-line.header-nav-top-line.header-nav-top-line-with-border.order-2.order-lg-1
                .header-nav-main.header-nav-main-square.header-nav-main-effect-2.header-nav-main-sub-effect-1
                  nav.collapse
                    ul#mainNav.nav.nav-pills
                      li.dropdown
                        a.dropdown-item.dropdown-toggle.text-primary(href='/')  Home 
                      li.dropdown
                        a.dropdown-item.dropdown-toggle.active-chan.active(href='/solver') Self-Solver
                      li.dropdown
                        a.dropdown-item.dropdown-toggle.text-primary(href='#') Compete
                      li.dropdown
                        a.dropdown-item.dropdown-toggle.text-primary(href='#') Discussion Panel
                      li.dropdown
                        a.dropdown-item.dropdown-toggle.text-primary(href='/contact') Analytics
                button.btn.header-btn-collapse-nav(data-toggle='collapse' data-target='.header-nav-main nav')
                  i.fa.fa-bars
              .header-nav-features.header-nav-features-no-border.header-nav-features-lg-show-border.order-1.order-lg-2
                .header-nav-feature.header-nav-features-search.d-inline-flex
                  a.header-nav-features-toggle(href='#' data-focus='headerSearch')
                    i.fas.fa-search.header-nav-top-icon
                  #headerTopSearchDropdown.header-nav-features-dropdown
                    form(role='search' action='#' method='get')
                      .simple-search.input-group
                        input#headerSearch.form-control.text-1(name='q' type='search' value='' placeholder='Search...')
                        span.input-group-append
                          button.btn(type='submit')
                            i.fa.fa-search.header-nav-top-icon
  // Navbar closing

